<section id="degree" style="background-color: whitesmoke">
    <div class="container">
        <div class="col-12 py-4 px-4">
            <h4 class="display-4">Parcours scolaire</h4>
            <hr>
        </div>
    </div>
    <div class="container-fluid py-5">

        <div class="row">
            <div class="col-lg-12">

                <div class="horizontal-timeline">

                    <ul class="list-inline items d-flex justify-content-center">
                        {% for degree in degrees %}
                            <li class="list-inline-item items-list">
                                <div class="px-4">
                                    <div class="event-date badge bg-info">{{ degree.startDate|date("m/Y") }} ->
                                        {{ degree.endDate|date("m/Y") }}</div>
                                    <h5 class="pt-2">{{ degree.school }}</h5>
                                    <p class="text-muted">{{ degree.name }}</p>
                                </div>
                            </li>
                        {% endfor %}
                    </ul>

                </div>

            </div>
        </div>

    </div>

</section>